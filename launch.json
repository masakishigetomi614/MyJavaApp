{
    // 使用可能なデバッグ構成のリスト
    "version": "0.2.0",
    "configurations": [
        {
            // この構成の名前
            "name": "Java: Launch",
            // 使用するタイプ
            "type": "java",
            // この構成でデバッグするメインクラス
            "mainClass": "TodoApp",
            // 任意: 追加の引数を指定する
            "args": [],
